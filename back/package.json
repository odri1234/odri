{
  "name": "internet-billing-system",
  "version": "1.0.0",
  "description": "Backend for the Internet Billing System",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/database/data-source.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/database/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/run-seeds.ts",
    "docker:build": "docker build -t billing-system .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "backup:db": "node dist/scripts/backup-db.js",
    "restore:db": "node dist/scripts/restore-db.js",
    "diagnose": "node scripts/diagnose.js",
    "fix:api": "node scripts/diagnose.js"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/cache-manager": "^2.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-ws": "^11.1.6",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.9",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^11.1.6",
    "archiver": "^6.0.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "cache-manager": "^5.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cron": "^2.4.0",
    "dotenv": "^16.3.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mikronode-ng": "^1.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdf-lib": "^1.17.0",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "speakeasy": "^2.0.0",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "webpack": "^5.101.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.3",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.0.0",
    "@types/archiver": "^6.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.1",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.0",
    "@types/qrcode": "^1.5.0",
    "@types/speakeasy": "^2.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
